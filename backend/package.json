{
	"name": "backend",
	"version": "1.0.0",
	"description": "v2 backend",
	"main": "./src/app.ts",
	"scripts": {
		"build": "tsc && terser ./dist -o ./dist && webpack",
		"compile": "tsc",
		"compile:live": "tsc -w",
		"format": "prettier --write .",
		"knex:migrate:make": "npx knex migrate:make RENAME -x ts --cwd ./src/services/postgres",
		"knex:migrate:latest": "npx knex migrate:latest --cwd ./src/services/postgres",
		"knex:migrate:rollback": "npx knex migrate:rollback --cwd ./src/services/postgres",
		"knex:seed": "npx knex seed:run --cwd ./src/services/postgres",
		"lint": "eslint . --ext .ts",
		"serve": "tsc && node ./.build/src/app.js",
		"test": "jest"
	},
	"author": "zak.jakub67@gmail.com",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.614.0",
		"@aws-sdk/s3-request-presigner": "^3.614.0",
		"body-parser": "^1.20.2",
		"connect-timeout": "^1.9.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-formidable": "^1.2.0",
		"knex": "^3.1.0",
		"multer": "^1.4.5-lts.1",
		"node-fetch": "^3.3.2",
		"pg": "8.4"
	},
	"devDependencies": {
		"@types/connect-timeout": "^0.0.39",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/express-formidable": "^1.2.3",
		"@types/jest": "^29.5.12",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.14.10",
		"@typescript-eslint/eslint-plugin": "^7.16.0",
		"@typescript-eslint/parser": "^7.16.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.3"
	}
}
